@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Eigen3 REQUIRED CONFIG)
if (@ALPAQA_WITH_CASADI@)
    find_package(casadi CONFIG)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/alpaqaTargets.cmake")

if(NOT _@PROJECT_NAME@_PRINTED)
    get_target_property(ALPAQA_CONFIG alpaqa::alpaqa IMPORTED_CONFIGURATIONS)
    message(STATUS "Found @PROJECT_NAME@ @PROJECT_VERSION@: ${CMAKE_CURRENT_LIST_DIR} (available configs: ${ALPAQA_CONFIG})")
    set(_@PROJECT_NAME@_PRINTED On CACHE INTERNAL "")
endif()
